{
  "name": "q2a.js",
  "version": "1.0.0",
  "main": "",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": "14.16.x"
  },
  "dependencies": {},
  "devDependencies": {
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4"
  },
  "lint-staged": {
    "*.test.js": "cd back_end && ./node_modules/.bin/jest --passWithNoTests",
    "*.js": "cd back_end && npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "install_packages": "yarn && cd front_end && yarn && cd ../back_end && yarn",
    "api_tests": "cd back_end && yarn test",
    "api_setup": "cd back_end && yarn setup",
    "api_run_dev": "cd back_end && yarn dev",
    "frontend_run_dev": "cd front_end && yarn dev",
    "deploy_docker": "docker-compose up -d",
    "docker_rebuild_frontend": " docker-compose up -d --no-deps --build frontend",
    "docker_rebuild_api": " docker-compose up -d --no-deps --build api",
    "prepare": "husky install"
  }
}
